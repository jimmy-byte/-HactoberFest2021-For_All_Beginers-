#include<bits/stdc++.h>
using namespace std;
void swap(int* a,int* b)
{
    int temp;
    temp=*a;
    *a=*b;
    *b=temp;
}
void bubblesort(int* arr,int n)
{
    for(int i=0;i<n-1;i++)

    {
        for(int j=0;j<n-i-1;j++)
        {
            if(arr[j+1]<arr[j])
                swap(&arr[j+1],&arr[j]);
        }
    }
}
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    bubblesort(arr,n);
    for(int i=0;i<n;i++)
        cout<<arr[i];
    return 0;
}
